{
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"arrowParens": "always",
	"bracketSameLine": true,
	"endOfLine": "auto",
	"semi": true,
	"singleQuote": true,
	"svelteAllowShorthand": false,
	"svelteSortOrder": "options-scripts-markup-styles",
	"svelteStrictMode": false,
	"tabWidth": 4,
	"trailingComma": "all",
	"useTabs": true,
	"printWidth": 80
}
